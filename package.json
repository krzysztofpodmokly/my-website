  },
  "homepage": "https://github.com/krzysztofpodmokly/my-website#readme",
  "devDependencies": {
    "autoprefixer": "^9.5.0",
    "concat": "^1.0.3",
    "node-sass": "^7.0.0",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "mongoose": "^5.5.4"
  }
}
