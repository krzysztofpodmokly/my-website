{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "watch:main": "sass --watch sass/main.scss public/css/style.css",
    "watch:projects": "sass --watch sass/projects.scss public/css/projects.css",
    "watch:form": "sass --watch sass/form.scss public/css/form.css",
    "watch:error": "sass --watch sass/error.scss public/css/error.css",
    "watch:nav": "sass --watch sass/layout/navigation.scss public/css/navigation.css",
    "watch:load": "sass --watch sass/layout/_loader.scss public/css/loader.css",
    "watch:curr": "sass --watch sass/curriculum.scss public/css/curriculum.css",
    "watch:foot": "sass --watch sass/layout/_footer.scss public/css/footer.css",
    "devserver": "live-server",
    "all": "npm-run-all --parallel watch:main watch:projects watch:form watch:error watch:nav watch:load watch:curr watch:foot",
    "dev": "nodemon app.js",
    "start": "NODE_ENV=production node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krzysztofpodmokly/my-website.git"
  },
